{
  "name": "percy-web",
  "version": "1.0.0",
  "description": "Percy web application",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build-test": "ember build --environment=test -o dist",
    "build-production": "yarn run clean && EMBER_ENV=production ember build --environment=production",
    "clean": "rm -rf dist/ tmp/",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js . --fix",
    "postinstall": "bower install --allow-root",
    "start": "ember server",
    "start-production-for-testing": "ember server --environment=production",
    "test": "ember test",
    "test:node_debug": "node debug node_modules/ember-cli/bin/ember test --server",
    "test:parallel": "EMBER_ENV=test percy-agent exec --network-idle-timeout 100 -- ember exam --environment=test --path dist --split=4 --parallel",
    "test:server": "ember test --server"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/eslint/bin/eslint.js --fix",
      "git add"
    ]
  },
  "repository": "percy/percy-web",
  "author": "Perceptual Inc.",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^0.6.1",
    "broccoli-asset-rev": "^3.0.0",
    "chai-as-promised": "^7.1.1",
    "ember-ajax": "^3.1.0",
    "ember-changeset": "^1.4.1",
    "ember-changeset-validations": "^1.2.9",
    "ember-cli": "~3.1.4",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "7.0.0",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-clipboard": "^0.9.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deprecation-workflow": "^1.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-flash": "^1.6.1",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.8.0",
    "ember-cli-meta-tags": "^5.1.0",
    "ember-cli-mirage": "^0.4.0",
    "ember-cli-mocha": "^0.14.4",
    "ember-cli-moment-shim": "^3.7.0",
