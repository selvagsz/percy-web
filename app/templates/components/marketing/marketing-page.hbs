{{#each page.blocks as |block|}}
  {{#if block.isContentBlock}}
    {{marketing/content-block block=block.contentBlock}}
  {{else if block.isQuoteBlock}}
    {{marketing/customer-quote-block quoteBlock=block.quoteBlock}}
  {{else if block.isFooter}}
    {{marketing/footer-cta content=block.footerCta}}
  {{else if block.isHero}}
    {{#marketing/marketing-header
      logomark=block.hero.logomark
      image=block.hero.mainImage
      classes=block.hero.classes}}
      {{marketing/hero-block content=block.hero}}
    {{/marketing/marketing-header}}
  {{else if block.isFaqBlock}}
    {{marketing/faq-block block=block.faqBlock}}
  {{else if block.isLogoBlock}}
    {{marketing/logo-block logos=block.logoBlock.customerLogos}}
  {{else if block.isProfileBlock}}
    {{marketing/profile-block profiles=block.profileBlock.profiles}}
  {{else if block.isPricingTableBlock}}
    {{marketing/pricing-table table=block.pricingTableBlock}}
  {{else if block.isPricingCardBlock}}
    {{marketing/pricing-card-block cards=block.pricingCardBlock.pricingCards}}
  {{else if block.isCaseStudyBlock}}
    {{marketing/case-study-block block=block.caseStudyBlock}}
  {{/if}}
{{/each}}
